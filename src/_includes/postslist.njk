<ul class="list-disc px-8">

  {% for post in postslist %}
    <li class="mb-8">
      <div>{{ post.data.date }}</div>
      <h2 class="text-2xl text-violet-700 mb-1">
        <a class="decoration-violet-700 underline decoration-solid" href="{{ post.url | url }}">{{ post.data.title }}</a></h2>
      <div>{{post.data.description}}</div>
    </li>
  {% endfor %}
</ul> 